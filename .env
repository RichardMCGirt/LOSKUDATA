PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
# Skip downloading Chromium during Puppeteer installation
PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true

# Specify the Puppeteer cache directory
PUPPETEER_CACHE_DIR=/tmp/puppeteer

# Optional: Add path to Chromium if using a system-installed version
PUPPETEER_EXECUTABLE_PATH=/usr/bin/chromium
